{
  "categories": [
    {
      "title": "## üöÄ Features",
      "labels": ["[feat]"]
    },
    {
      "title": "## üêõ Fixes",
      "labels": ["[fixed]"]
    },
    {
      "title": "## ‚¨Ü Update",
      "labels": ["[changed]"]
    },
    {
      "title": "## ‚ú® Added",
      "labels": ["[added]"]
    },
    {
      "title": "## \uD83D\uDDD1Ô∏è Remove",
      "labels": ["[removed]"]
    }
  ],
  "ignore_labels": ["ignore"],
  "sort": "DESC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}}",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "flags": "gu"
    },
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "on_property": "title"
    }
  ],
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "tag_resolver": {
    "method": "semver"
  }
}
